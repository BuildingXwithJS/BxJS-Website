{
  "name": "codezen-hasura",
  "domain": "Host(`graphql.bxjs.dev`, `graphql.codezen.dev`, `graphql.databorg.ai`)",
  "port": 8080,
  "env": {
    "HASURA_GRAPHQL_DATABASE_URL": "postgres://postgres:postgrespassword@bxjspostgres:5432/postgres",
    "HASURA_GRAPHQL_ENABLE_CONSOLE": "false",
    "HASURA_GRAPHQL_DEV_MODE": "false",
    "HASURA_GRAPHQL_ENABLED_LOG_TYPES": "startup, http-log, webhook-log, websocket-log, query-log",
    "HASURA_GRAPHQL_ADMIN_SECRET": "@bxjs-hasura-token",
    "HASURA_GRAPHQL_UNAUTHORIZED_ROLE": "anonymous",
    "HASURA_GRAPHQL_JWT_SECRET": "@bxjs-hasura-jwt-secret",
    "ACTION_BASE_URL": "https://bxjs.dev"
  }
}
