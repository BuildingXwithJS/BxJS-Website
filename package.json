{
  "name": "bxjs-website",
  "description": "BxJS Website. Includes AI to auto-generate BxJS Weekly podcast episodes",
  "version": "1.0.0",
  "author": "Tim Ermilov <yamalight@gmail.com>",
  "private": true,
  "dependencies": {
    "fastify": "^3.14.0",
    "next": "^10.0.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "keywords": [
    "bxjs",
    "next",
    "tailwindcss",
    "fastify",
    "tensorflow"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different '**/*.{js,css,md}'",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md}'",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "next build",
    "dev": "node ./server/index.js",
    "start": "NODE_ENV=production node ./server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BuildingXwithJS/BxJS-Website"
  },
  "bugs": {
    "url": "https://github.com/BuildingXwithJS/BxJS-Website/issues"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.10",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "prettier": "^2.2.1"
  }
}
